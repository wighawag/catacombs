		"@scure/base": "^1.2.1",
		"@scure/bip39": "^1.5.0",
		"eip-1193-jsonrpc-provider": "^0.4.2",
		"eip-1193-signer": "^0.1.0",
		"ethereum-indexer-browser": "^0.7.7",
		"ethereum-tx-observer": "^0.0.5",
		"focus-trap": "^7.6.2",
		"fuzd-scheduler": "^0.7.2",
		"lz-string": "^1.5.0",
		"named-logs": "^0.3.0",
		"pretty-ms": "^9.2.0",
		"radiate": "^0.0.2",
		"remote-account": "^0.2.1",
		"template-game-common": "workspace:*",
		"template-game-contracts-js": "workspace:*",
		"template-game-indexer": "workspace:*",
		"tlock-js": "^0.9.0",
		"twgl.js": "^5.5.4",
